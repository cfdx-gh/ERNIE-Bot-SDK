{
	"name": "searchDomainWhoisInfo",
	"description": "获取某个域名的相关信息",
	"parameters": {
		"type": "object",
		"properties": {
			"domain": {
				"type": "string",
				"description": "要查询的域名"
			}
		},
		"required": ["domain"]
	},
	"responses": {
		"type": "object",
		"properties": {
			"Registrant": {
				"type": "string",
				"description": "注册商"
			},
			"Holder": {
				"type": "string",
				"description": "域名持有人/机构名称"
			},
			"DomainServer": {
				"type": "string",
				"description": "域名服务器"
			},
			"DnsServer": {
				"type": "string",
				"description": "DNS服务器"
			},
			"DomainStatus": {
				"type": "string",
				"description": "域名状态"
			},
			"RegistrationTime": {
				"type": "string",
				"description": "创建时间"
			},
			"ExpirationTime": {
				"type": "string",
				"description": "过期时间"
			}
		},
		"required": ["Registrant", "Holder", "DomainServer", "DnsServer", "DomainStatus", "RegistrationTime", "ExpirationTime"]
	}
}
