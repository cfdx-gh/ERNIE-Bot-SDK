{
	"name": "queryDictWordMean",
	"description": "查询某个字的详细解释",
	"parameters": {
		"type": "object",
		"properties": {
			"word": {
				"type": "string",
				"description": "要查询的字"
			}
		},
		"required": ["word"]
	},
	"responses": {
		"type": "object",
		"properties": {
			"pinyin": {
				"type": "string",
				"description": "要查询字的拼音"
			},
			"bihua": {
				"type": "string",
				"description": "要查询字的笔画"
			},
			"bushou": {
				"type": "string",
				"description": "要查询字的部首"
			},
			"words": {
				"type": "string",
				"description": "要查询字的组词"
			},
			"basic_explain": {
				"type": "string",
				"description": "要查询字的基本解释"
			}
		},
		"required": ["pinyin", "bihua", "bushou", "words", "basic_explain"]
	}
}
