{
	"name": "queryIpAddressInfo",
	"description": "查询某个IP地址信息",
	"parameters": {
		"type": "object",
		"properties": {
			"ip": {
				"type": "string",
				"description": "要查询的IP地址"
			}
		},
		"required": ["ip"]
	},
	"responses": {
		"type": "object",
		"properties": {
			"disp": {
				"type": "string",
				"description": "IP地址所在城市以及运营商名称"
			},
			"start_ip": {
				"type": "string",
				"description": "该IP所属起始IP是多少"
			},
			"end_ip": {
				"type": "string",
				"description": "该IP所属最大范围是多少"
			}
		},
		"required": ["disp", "start_ip", "end_ip"]
	}
}
