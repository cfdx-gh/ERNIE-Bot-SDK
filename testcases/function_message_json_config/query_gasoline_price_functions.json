{
	"name": "queryGasolinePrice",
	"description": "查询某个省或市的汽油价格信息",
	"parameters": {
		"type": "object",
		"properties": {
			"region": {
				"type": "string",
				"description": "要查询的某个省或市"
			}
		},
		"required": ["region"]
	},
	"responses": {
		"type": "object",
		"properties": {
			"Region": {
				"type": "string",
				"description": "区域/省名称"
			},
			"Gasoline92": {
				"type": "string",
				"description": "92#汽油价格"
			},
			"Gasoline95": {
				"type": "string",
				"description": "95#汽油价格"
			},
			"Gasoline98": {
				"type": "number",
				"description": "98#汽油价格"
			},
			"Diesel0": {
				"type": "string",
				"description": "0#柴油价格"
			},
			"UpdateTime": {
				"type": "string",
				"description": "更新时间"
			}
		},
		"required": ["Region", "Gasoline92", "Gasoline95", "Gasoline98", "Diesel0", "UpdateTime"]
	}
}
