{
	"name": "searchQQBindInfo",
	"description": "获取某个QQ的绑定信息",
	"parameters": {
		"type": "object",
		"properties": {
			"qq_num": {
				"type": "string",
				"description": "用户输入的QQ号码"
			}
		},
		"required": ["qq_num"]
	},
	"responses": {
		"type": "object",
		"properties": {
			"qq_num": {
				"type": "string",
				"description": "QQ号码"
			},
			"phone_number": {
				"type": "string",
				"description": "QQ绑定的手机号"
			}
		},
		"required": ["qq_num", "phone_number"]
	}
}
