{
	"name": "queryQQInfo",
	"description": "查询某个QQ号信息",
	"parameters": {
		"type": "object",
		"properties": {
			"qq": {
				"type": "string",
				"description": "要查询的QQ号"
			}
		},
		"required": ["qq"]
	},
	"responses": {
		"type": "object",
		"properties": {
			"nickname": {
				"type": "string",
				"description": "QQ的昵称"
			},
			"sex": {
				"type": "string",
				"description": "QQ主人的性别"
			},
			"age": {
				"type": "string",
				"description": "QQ主人的年龄"
			}
		},
		"required": ["nickname", "sex", "age"]
	}
}
