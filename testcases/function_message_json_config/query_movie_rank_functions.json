{
	"name": "queryMovieRank",
	"description": "查询电影排行信息",
	"parameters": {
		"type": "object",
		"properties": {
			"count": {
				"type": "string",
				"description": "要显示的数量数量"
			}
		},
		"required": ["count"]
	},
	"responses": {
		"type": "object",
		"properties": {
			"title": {
				"type": "string",
				"description": "电影名称"
			},
			"movie_url": {
				"type": "string",
				"description": "电影网址链接"
			},
			"movie_imgurl": {
				"type": "string",
				"description": "电影图片链接"
			},
			"movie_actor": {
				"type": "number",
				"description": "电影参与演员"
			},
			"movie_score": {
				"type": "string",
				"description": "电影评分"
			}
		},
		"required": ["title", "movie_url", "movie_imgurl", "movie_actor", "movie_score"]
	}
}
