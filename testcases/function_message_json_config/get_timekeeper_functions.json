{
	"name": "getTimeKeeper",
	"description": "获取整点报时信息",
	"parameters": {
		"type": "object",
		"properties": {
			"hour": {
				"type": "string",
				"description": "需要报时的整点，比如晚上22点"
			}
		},
		"required": ["hour"]
	},
	"responses": {
		"type": "object",
		"properties": {
			"tip_msg": {
				"type": "string",
				"description": "提示文字"
			},
			"tip_url": {
				"type": "string",
				"description": "提示语音链接"
			}
		},
		"required": ["tip_msg", "tip_url"]
	}
}
