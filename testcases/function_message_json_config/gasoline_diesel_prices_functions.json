{
	"name": "getGasolineDieselPrices",
	"description": "获取某一个州或省内各个城市的汽油和柴油价格",
	"parameters": {
		"type": "object",
		"properties": {
			"state": {
				"type": "string",
				"description": "州名称"
			}
		},
		"required": ["state"]
	},
	"responses": {
		"type": "object",
		"properties": {
			"name": {
				"type": "string",
				"description": "州名称"
			},
			"gasoline": {
				"type": "number",
				"description": "汽油价格"
			},
			"diesel": {
				"type": "number",
				"description": "柴油价格"
			}
		},
		"required": ["name", "gasoline", "diesel"]
	}
}
