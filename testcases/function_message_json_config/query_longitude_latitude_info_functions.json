{
	"name": "queryLongitudeAndLatitudeInfo",
	"description": "查询经纬度的详细信息",
	"parameters": {
		"type": "object",
		"properties": {
			"longitude": {
				"type": "string",
				"description": "要查询的经度"
			},
			"latitude": {
				"type": "string",
				"description": "要查询的纬度"
			}
		},
		"required": ["longitude", "latitude"]
	},
	"responses": {
		"type": "object",
		"properties": {
			"address": {
				"type": "string",
				"description": "经纬度对应的地址"
			},
			"formatted_addresses": {
				"type": "string",
				"description": "经纬度对应的格式化地址信息"
			},
			"address_component": {
				"type": "string",
				"description": "经纬度对应的详细地址"
			},
			"ad_info": {
				"type": "string",
				"description": "经纬度对应的地址其他信息"
			}
		},
		"required": ["address", "formatted_addresses", "address_component", "ad_info"]
	}
}
