{
	"name": "getEncryMd5",
	"description": "获取某段文字的加密MD5",
	"parameters": {
		"type": "object",
		"properties": {
			"text": {
				"type": "string",
				"description": "需要加密的一段文字"
			}
		},
		"required": ["text"]
	},
	"responses": {
		"type": "object",
		"properties": {
			"md5": {
				"type": "string",
				"description": "加密后的MD5"
			}
		},
		"required": ["md5"]
	}
}
