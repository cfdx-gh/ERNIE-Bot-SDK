{
	"name": "queryDomainEmploy",
	"description": "查询某个域名的收录与反链信息",
	"parameters": {
		"type": "object",
		"properties": {
			"domain": {
				"type": "string",
				"description": "要查询的域名"
			}
		},
		"required": ["domain"]
	},
	"responses": {
		"type": "object",
		"properties": {
			"record": {
				"type": "string",
				"description": "收录信息"
			},
			"anti": {
				"type": "string",
				"description": "反链信息"
			},
			"update": {
				"type": "string",
				"description": "数据更新时间"
			}
		},
		"required": ["record", "anti", "update"]
	}
}
