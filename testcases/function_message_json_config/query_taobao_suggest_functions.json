{
	"name": "queryTaobaoSuggestList",
	"description": "查询某个关键词在淘宝上有哪些搜索选项",
	"parameters": {
		"type": "object",
		"properties": {
			"keyword": {
				"type": "string",
				"description": "要查询的关键词"
			}
		},
		"required": ["keyword"]
	},
	"responses": {
		"type": "object",
		"properties": {
			"search_name": {
				"type": "string",
				"description": "搜索名称"
			},
			"search_index": {
				"type": "string",
				"description": "搜索指数"
			}
		},
		"required": ["search_name", "search_index"]
	}
}
